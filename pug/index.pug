    +js_product_detail()
    script.
        $(document).ready(function(){
            numSlick = 0;
            sliderList(".products-list-row1", 5, 4, 1);
            sliderList(".products-list-row2", 5, 4, 2);
            sliderList(".slick-news ", 4, 4, 1);
            sliderSmallItem(".slick-news-22 ", 2, 1, 2);
            sliderSmallItem(".slick-news-23 ", 2, 1, 3);
            sliderSmallItem(".slick-news-24 ", 2, 1, 4);

            galleryImages(".slider-gallery-row2", 6, 5, 2);
            galleryImages(".slider-gallery-column6", 6, 5, 1);
            galleryImages(".slider-gallery-column8", 8, 5, 1);
            galleryImages(".slider-gallery-column4", 4, 4, 1);
            function galleryImages($sliderName, $toshow, $toshow_1024, $rows) {
                $($sliderName).each( function() {
                    numSlick++;
                    $(this).addClass( 'slider-' + numSlick ).slick({
                        slidesToShow: $toshow,
                        rows: $rows,
                        slidesToScroll: 1,
                        arrow: true,
                        autoplay: true,
                        pauseOnHover: true,
                        //- rtl: true,
                        responsive: [
                        {
                            breakpoint: 1024,
                            settings: {
                            slidesToShow: $toshow_1024,
                            }
                        },{
                            breakpoint: 991,
                            settings: {
                            slidesToShow: 4,
                            }
                        },{
                            breakpoint: 768,
                            settings: {
                            slidesToShow: 3,
                            }
                        },{
                            breakpoint: 576,
                            settings: {
                            slidesToShow: 2,
                            }
                        }
                        ]
                    });
                    $(this).addClass( 'slider-' + numSlick ).slickLightbox({
                        itemSelector: 'a',
                        //- src: 'img', // lấy ảnh từ <img>
                        captions: true,
                        caption: function (el) {
                            return $(el).find('.content').html() || '';
                        },
                        captionPosition: 'bottom'
                    });
                });
            };
        });

            
